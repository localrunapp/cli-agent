{
  "name": "localrun-cli-agent",
  "version": "0.1.11",
  "description": "LocalRun Agent - MultiOs service for local development",
  "author": "LocalRun Team",
  "bin": {
    "localrun": "./bin/run.js"
  },
  "homepage": "https://github.com/localrunapp/cli-agent",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "localrunapp/cli-agent",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "@oclif/plugin-update": "^4.7.16",
    "@types/node-fetch": "^2.6.13",
    "chalk": "^4.1.2",
    "dockerode": "^4.0.0",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "node-pty": "^1.0.0",
    "ora": "^5.4.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/dockerode": "^3.3.47",
    "@types/express": "^4.17.21",
    "@types/mocha": "^9",
    "@types/node": "^18",
    "@types/ws": "^8.18.1",
    "chai": "^4",
    "eslint": "^8.54.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "oclif": {
    "bin": "localrun",
    "dirname": "localrun",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-update"
    ],
    "topicSeparator": " ",
    "topics": {},
    "macos": {
      "identifier": "com.localrun.agent"
    },
    "win": {
      "identifier": "com.localrun.agent"
    },
    "linux": {
      "executable": "localrun"
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "dev": "ts-node --transpile-only src/index.ts",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "pack:macos": "oclif pack macos",
    "pack:win": "oclif pack win",
    "pack:tarballs": "oclif pack tarballs --targets darwin-arm64,darwin-x64,linux-arm64,linux-x64,win32-x64"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "bugs": "https://github.com/localrunapp/cli-agent/issues",
  "keywords": [
    "oclif",
    "cli",
    "localrun",
    "agent",
    "macos"
  ],
  "types": "dist/index.d.ts"
}
